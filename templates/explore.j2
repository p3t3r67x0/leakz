<script>
	var app = new Vue({
		el: '#app',
		data: {
			showNav: false
		},
		methods: {
			hideNav() {
				return this.showNav = false
			},
			toggleNav() {
				return this.showNav = !this.showNav
			}
		}
	})
</script>
